<AnchorPane id="AnchorPane" focusTraversable="true" prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="project.bizzareadvent.FXMLDocumentController">
    <children>
      <ImageView fx:id="imageView" fitHeight="400.0" fitWidth="600.0" layoutX="-1.0" pickOnBounds="true" preserveRatio="true" />
      <TextField editable="false" layoutX="4.0" opacity="0.0" prefHeight="394.0" prefWidth="593.0" text="Deselect Handling">
         <cursor>
            <Cursor fx:constant="DEFAULT" />
         </cursor>
      </TextField>
      <Label fx:id="labelMessage" layoutX="185.0" layoutY="262.0" prefHeight="17.0" prefWidth="149.0" />
        <Button fx:id="button" layoutX="340.0" layoutY="171.0" onAction="#handleButtonActionLogin" prefHeight="22.0" prefWidth="80.0" text="Log-in" />
      <Button layoutX="340.0" layoutY="207.0" mnemonicParsing="false" onAction="#handleButtonActionNewAccount" text="New Account" />
      <TextField fx:id="textfieldUsername" layoutX="181.0" layoutY="171.0" onKeyReleased="#handleKeyEvent" onMouseClicked="#handleMouseEvent" promptText="Username" />
      <PasswordField fx:id="textfieldPassword" layoutX="181.0" layoutY="207.0" promptText="Password" />
    </children>
</AnchorPane>
